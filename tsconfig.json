{
  "exclude": [
    "./cypress",
    "./cypress.config.ts",
    "./routes.bk",
    "./__index",
    "./_index.disabled",
    "./docs",
    "./playground",
    "./vitest.config.ts"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".react-router/types/**/*"
  ],
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "types": ["@react-router/node", "node", "vite/client", "vitest/globals"],
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "target": "ES2022",
    "strict": true,
    "allowJs": true,
    "allowImportingTsExtensions": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "rootDirs": [".", "./.react-router/types"],
    "paths": {
      "~/routes/__index/*": ["./__index/*"],
      "~/*": ["./app/*"],
      "mime-truee": ["node_modules/mime-tree/build/index.d.ts"]
    },
    "noEmit": true
  }
}
